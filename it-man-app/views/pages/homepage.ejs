<h1>Student Management</h1>

  <div id="students">
    <!-- Lista de estudantes serÃ¡ inserida aqui -->
  </div>
  <button id="load-students">Load Students</button>

  <script>
    $(document).ready(function () {
      $('#load-students').click(function () {
        $.get('/student/read', function (data) {
          $('#students').empty();
          data.forEach(function (student) {
            $('#students').append(`<p>${student.name} - ${student.email}</p>`);
          });
        });
      });
    });
  </script>